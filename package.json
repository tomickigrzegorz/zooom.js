{
  "name": "zooom",
  "version": "1.0.9",
  "description": "The easiest way to zooming a photo",
  "main": "docs/zooom.min.js",
  "browser": "docs/zooom.umd.min.js",
  "author": "Grzegorz Tomicki",
  "license": "MIT",
  "homepage": "https://github.com/tomik23/zooom.js",
  "scripts": {
    "dev": "concurrently \"yarn style:watch\" \"yarn watch\"",
    "prod": "yarn style:prod && yarn build",
    "build": "rollup -c --environment PRODUCTION",
    "watch": "rollup -c -w",
    "style:watch": "node-sass -w sources/scss -o docs",
    "style:prod": "node-sass sources/scss -o docs --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tomik23/zooom.js.git"
  },
  "keywords": [
    "zoom",
    "image"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-typescript": "^6.1.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.0",
    "rollup": "^2.33.3",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}